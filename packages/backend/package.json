{
  "name": "autodroid-api",
  "author": "luizfelipelaviola",
  "version": "0.0.1",
  "main": "./src/index.ts",
  "license": "MIT",
  "engines": {
    "node": "v20.14.0"
  },
  "scripts": {
    "dev": "nodemon",
    "dev:server": "node -r ts-node/register -r tsconfig-paths/register ./src/index.ts",
    "start:dev": "node src/index.js -e development",
    "start:staging": "node src/index.js -e staging",
    "start:prod": "node src/index.js -e production",
    "run-ts": "node -r ts-node/register -r reflect-metadata -r dotenv/config -r tsconfig-paths/register",
    "lint": "run-p -c lint:*",
    "lint:eslint": "eslint 'src/**/*.{ts,tsx}' --fix",
    "lint:tsc": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:dev": "prisma migrate dev",
    "prisma:prod": "prisma migrate deploy",
    "test": "vitest run --config test/config/vitest.config.ts",
    "test:watch": "vitest --config test/config/vitest.config.ts",
    "test:debug": "node --inspect ./node_modules/.bin/vitest --config test/config/vitest.config.ts --no-file-parallelism --testTimeout 60000",
    "test:unit": "vitest run --config test/config/vitest.config.ts --project unit",
    "test:unit:watch": "vitest --config test/config/vitest.config.ts --project unit",
    "test:unit:debug": "node --inspect ./node_modules/.bin/vitest run --config test/config/vitest.config.ts --project unit --no-file-parallelism --testTimeout 60000",
    "test:integration": "vitest run --config test/config/vitest.config.ts --project integration",
    "test:integration:watch": "vitest --config test/config/vitest.config.ts --project integration",
    "test:integration:debug": "node --inspect ./node_modules/.bin/vitest run --config test/config/vitest.config.ts --project integration --no-file-parallelism --testTimeout 60000",
    "i18n": "i18next-scanner --config src/shared/i18n/extraction.js --output src/shared/i18n/extraction 'src/**/*.{ts,tsx,js,jsx}'",
    "changelog": "commit-and-tag-version --skip.bump --skip.commit --skip.tag -i ./docs/CHANGELOG.md",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "@prisma/client": "^5.15.0",
    "@sentry/node": "^8.9.2",
    "@sentry/profiling-node": "^8.9.2",
    "@socket.io/redis-adapter": "^8.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "2.30.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-useragent": "^1.0.15",
    "firebase-admin": "^12.1.1",
    "geoip-lite": "^1.4.10",
    "graphql": "^16.8.2",
    "graphql-query-complexity": "^1.0.0",
    "graphql-scalars": "^1.23.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.1.0",
    "i18next": "^23.11.5",
    "i18next-fs-backend": "^2.3.1",
    "i18next-http-middleware": "^3.6.0",
    "ioredis": "^5.4.1",
    "is-language-code": "3.1.0",
    "mongoose": "^8.4.3",
    "pg": "^8.12.0",
    "prisma": "^5.15.0",
    "prisma-dbml-generator": "^0.12.0",
    "rate-limiter-flexible": "^5.0.3",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.6.0",
    "socket.io": "^4.7.5",
    "socket.io-adapter": "^2.5.5",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.8.0",
    "type-graphql": "^2.0.0-rc.1",
    "validator": "^13.12.0",
    "yargs": "^17.7.2",
    "youch": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@swc/core": "^1.6.1",
    "@testcontainers/mongodb": "^10.9.0",
    "@testcontainers/postgresql": "^10.9.0",
    "@testcontainers/redis": "^10.9.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express-useragent": "^1.0.5",
    "@types/geoip-lite": "^1.4.4",
    "@types/node": "^20.14.3",
    "@types/semver": "^7.5.8",
    "@types/validator": "^13.12.0",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "commit-and-tag-version": "^12.4.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unicorn": "^54.0.0",
    "husky": "^9.0.11",
    "i18next-scanner": "^4.5.0",
    "lint-staged": "^15.2.7",
    "nodemon": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "regenerator-runtime": "^0.14.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-swc": "^1.4.5",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.6.0"
  },
  "nodemonConfig": {
    "watch": [
      "src/**/*"
    ],
    "ext": "ts,json",
    "exec": "yarn dev:server"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
